		"slug": "book-store",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/logo.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
